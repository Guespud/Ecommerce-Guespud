 setCounter] = useState(0);\n\n  const onAdd = () => {\n    setData({\n      ...data,\n      cantidad: data.cantidad + counter,\n      items: [...data.items, item],\n    });\n    history.push(\"/cart\");\n    // alert(`Agregaste ${counter} productos al carrito`);\n  };\n\n  const handleAdd = () => {\n    setCounter(counter + 1);\n  };\n\n  const handleSubtract = () => {\n    if (counter > 0) {\n      setCounter(counter - 1);\n    } else {\n      alert(\"0 es el valor minimo\");\n    }\n  };\n\n  return (\n    <>\n      <section className=\"text-center my-5\">\n        <h2 className=\"h1-responsive font-weight-bold text-center my-5\">\n          {item.title}\n        </h2>\n        {/* <p className=\"grey-text text-center w-responsive mx-auto mb-5\">\n        Lorem ipsum dolor sit amet, consectetur adipisicing elit. Fugit,\n        error amet numquam iure provident voluptate esse quasi, veritatis\n        totam voluptas nostrum quisquam eum porro a pariatur veniam.\n      </p> */}\n        <MDBRow>\n          <MDBCol lg=\"12\" md=\"6\" className=\"mb-lg-0 mb-4\">\n            <MDBCard className=\"align-items-center\">\n              <MDBCardImage\n                src={item.img}\n                top\n                alt=\"sample photo\"\n                overlay=\"white-slight\"\n              />\n              <MDBCardBody className=\"text-center\">\n                <a href=\"#!\" className=\"grey-text\">\n                  <h5>Shirt</h5>\n                </a>\n                <h5>\n                  <strong>\n                    <a href=\"#!\" className=\"dark-grey-text\">\n                      {item.title}{\" \"}\n                      <MDBBadge pill color=\"danger\">\n                        NEW\n                      </MDBBadge>\n                    </a>\n                  </strong>\n                </h5>\n                <h4 className=\"font-weight-bold blue-text\">\n                  <strong>${item.price}</strong>\n                </h4>\n                {/* <CountItem /> */}\n                <MDBRow center>\n                  <div className=\"def-number-input number-input\">\n                    <button\n                      onClick={handleSubtract}\n                      className=\"minus\"\n                      disabled={counter === 0 ? \"disabled\" : null}\n                    ></button>\n                    <input\n                      className=\"quantity\"\n                      name=\"quantity\"\n                      value={counter}\n                      onChange={() => console.log(\"change\")}\n                      type=\"number\"\n                    />\n                    <button\n                      onClick={handleAdd}\n                      className=\"plus\"\n                      disabled={counter === Stock ? \"disabled\" : null}\n                    ></button>\n                  </div>\n                </MDBRow>\n                <MDBBtn\n                  color=\"default\"\n                  onClick={onAdd}\n                  disabled={counter === 0 ? \"disabled\" : null}\n                >\n                  <MDBIcon icon=\"tshirt\" className=\"mr-1\" /> Agregar Al Carrito\n                </MDBBtn>\n                <br />\n              </MDBCardBody>\n            </MDBCard>\n          </MDBCol>\n        </MDBRow>\n      </section>\n    </>\n  );\n};\n\nexport default ProductDetail;\n","/Users/alejandroguespud/Documents/GitHub/Ecommerce-Guespud/src/store/index.js",[],"/Users/alejandroguespud/Documents/GitHub/Ecommerce-Guespud/src/components/menu/navbar/CartSlider.jsx",[],"/Users/alejandroguespud/Documents/GitHub/Ecommerce-Guespud/src/components/Cart/index.jsx",["108"],{"ruleId":"109","replacedBy":"110"},{"ruleId":"111","replacedBy":"112"},{"ruleId":"113","severity":1,"message":"114","line":64,"column":4,"nodeType":"115","endLine":64,"endColumn":6,"suggestions":"116"},{"ruleId":"117","severity":1,"message":"118","line":6,"column":8,"nodeType":"119","messageId":"120","endLine":6,"endColumn":17},{"r